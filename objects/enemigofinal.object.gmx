            <string>switch(state){
    case "inicial":
        // Moverse hacia el punto inicial y luego atacar
        if (point_distance(x, y, room_width/2,128) &gt; 1)
            move_towards_point(room_width/2,128,1);
        else {
            speed = 0;
            state = "atacar";
        }
        break;
    case "atacar":
        // Disparar mientras se mueve
        if(alarm[0]==-1) {
            disparo = instance_create(x,y+172,objbalamia);
            disparo.vspeed = 2;
            alarm[0] = room_speed/1.5; // Pr칩ximo disparo en..
        }
        // Movimiento
        x += move*2;
        if (x&lt;96) move = 1;
        else if (x&gt;room_width-96) move = -1;
        if (hp_actual &lt; hp/2) state = "centro";
        break;
    case "centro":
        // Volver al centro y berserker
        image_blend = c_red;
        if point_distance(x, y, room_width/2,128) &gt; 4
            move_towards_point(room_width/2,128,4);
        else {
            speed = 0;
            state = "berserker";
        }
        break;
    case "berserker":
        // Disparar r치fagas
        if(alarm[0]==-1) {
            for(var i=0;i&lt;16;i++){
                disparo = instance_create(x+96,y+96,obj_balaenemigo);
                disparo.speed = 2;
                disparo.direction = 360/16 * i;
            }
            alarm[0] = room_speed*2; // Pr칩xima r치faga en...
        }
        break;
    
}
